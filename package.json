{
  "name": "reactexpressboilerplate",
  "version": "1.0.0",
  "description": "Starter pack for ReactJS and Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"mongod\" \"npm run server\" \"npm run client\"",
    "yarn": "concurrently \"yarn\" \"cd client && yarn\""
  },
  "author": "Ibrohim Bahromov",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongojs": "^2.6.0",
    "nodemailer": "^4.6.7"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "nodemon": "^1.17.5"
  }
}
