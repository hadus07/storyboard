{
  "name": "reactexpressboilerplate",
  "version": "1.0.0",
  "description": "Starter pack for ReactJS and Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "yarn": "yarn && cd client && yarn"
  },
  "author": "Ibrohim Bahromov",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
